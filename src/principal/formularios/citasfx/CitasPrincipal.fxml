      <Button fx:id="btnComprobar" layoutX="515.0" layoutY="36.0" mnemonicParsing="false" onMouseClicked="#ComprobarCitasHoy" prefHeight="25.0" prefWidth="88.0" text="Comprobar" />

    </children>
</AnchorPane>
