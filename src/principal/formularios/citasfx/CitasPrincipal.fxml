                <TableColumn fx:id="tcHora" prefWidth="83.0" text="Hora" />
                <TableColumn fx:id="tcFecha" prefWidth="108.0" text="fecha" />
                <TableColumn fx:id="tcTratamientos" prefWidth="152.0" text="Tratamientos" />
                <TableColumn fx:id="tcDuracion" prefWidth="68.0" text="Duracion" />
            </columns>
        </TableView>
        <Button fx:id="btnNuevaCita" layoutX="56.0" layoutY="360.0" mnemonicParsing="false" onMouseClicked="#IraNuevaCita" prefHeight="34.0" prefWidth="88.0" text="Nueva Cita" />
